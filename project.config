[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group ldap/acumos-gerrit-vm-predictor-committers
	forgeCommitter = group ldap/acumos-gerrit-vm-predictor-committers
	push = +force group ldap/acumos-gerrit-vm-predictor-committers
	pushMerge = group ldap/acumos-gerrit-vm-predictor-committers
[receive]
	requireSignedOffBy = false
	requireChangeId = false
	enableSignedPush = true
	requireSignedPush = false
